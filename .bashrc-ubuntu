# Start dmux on default
[[ -z "$TMUX"  ]] && exec tmux

# --------
# Aliases
# --------
alias v="~/nvim.appimage"

alias dcst="sudo docker-compose start"
alias dcu="sudo docker-compose up -d"
alias dcsp="sudo docker-compose stop"
alias dcd="sudo docker-compose down"
alias dcb="sudo docker-compose build"

export VISUAL=vim
export EDITOR="$VISUAL"

# 2 rocket ships -> pwd in color blue, type in white
